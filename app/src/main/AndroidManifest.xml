<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kulzdev.bubblesproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.ServicesActivity" />
        <activity android:name=".Activities.ClientListActivity"/>
        <activity android:name=".Activities.ClientProfileActivity" />
        <activity
            android:name=".Activities.ClientHomeActivity"
            android:label="@string/title_activity_client_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activities.RegistrationActivity" />
        <activity android:name=".Activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>